
<table class="table">
    <thead>
        <th>
            <td>Id</td>
            <td>Name</td>
            <td>Price</td>
        </th>                  
    </thead>    
    <tbody>
        @foreach (var item in ViewBag.Products)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
            </tr>
        }
        
    </tbody>
</table>